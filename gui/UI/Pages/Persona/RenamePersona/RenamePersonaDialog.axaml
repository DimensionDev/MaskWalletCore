<controls:Dialog
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:controls="clr-namespace:Dimension.MaskCore.Lifecycle.Controls"
    xmlns:renamePersona="clr-namespace:Dimension.MaskCore.UI.Pages.Persona.RenamePersona"
    mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
    Title="Rename Persona"
    CloseButtonText="Cancel"
    PrimaryButtonText="Rename"
    PrimaryButtonCommand="{CompiledBinding RenameCommand}"
    x:DataType="renamePersona:RenamePersonaViewModel"
    x:Class="Dimension.MaskCore.UI.Pages.Persona.RenamePersona.RenamePersonaDialog">
    <controls:Dialog.DataContext>
        <renamePersona:RenamePersonaViewModel />
    </controls:Dialog.DataContext>
    <TextBox Text="{CompiledBinding Name, Mode=TwoWay}" />
</controls:Dialog>